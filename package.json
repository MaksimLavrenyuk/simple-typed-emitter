{
  "name": "simple-typed-emitter",
  "version": "0.0.0",
  "description": "Simple, typed, event emitter",
  "main": "dist/index",
  "typings": "dist/index",
  "scripts": {
    "start": "NODE_ENV=dev yarn install --check-files && rollup -c -w --watch.skipWrite",
    "build": "NODE_ENV=prod rollup -c"
  },
